	public static void main(String[] args) {

		// Creacion de las 3 Variables
		Producto productoA;
		Producto productoB;
		Producto productoC;

		// Modificacion de los valores Producto A
		productoA.nombre = "Papitas";
		productoA.descripcion = "Snack";
		productoA.precio = 0.35;
		productoA.stockActual = 20;

		
		// Modificacion de los valores Producto A
		productoB.nombre = "Caramelos";
		productoB.descripcion = "dulces";
		productoB.precio = 0.10;
		productoB.stockActual = 50;

		
		// Modificacion de los valores Producto A
		productoC.nombre = "Azucar";
		productoC.descripcion = "Viveres";
		productoC.precio = 1.00;
		productoC.stockActual = 45;

		
	}

}
